    <Style x:Key="stackPanelMain2" TargetType="{x:Type StackPanel}">
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Orientation" Value="Vertical" />
        <Setter Property="Grid.Column" Value="1" />
